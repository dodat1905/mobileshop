<%= render "sidebar" %>
<div class="col-md-9">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">Website Overview</h3>
    </div>
  <div class="panel-body">
    <div class="col-md-3">
      <div class="well dash-box">
        <h2><span class="glyphicon glyphicon-user" aria-hidden="true"></span> <%= @supports.users.count %></h2>
        <h4>Users</h4>
     </div>
   </div>
   <div class="col-md-3">
     <div class="well dash-box">
       <h2><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span> <%= @supports.brands.count %></h2>
       <h4>Brands</h4>
     </div>
   </div>
   <div class="col-md-3">
     <div class="well dash-box">
       <h2><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> <%= @supports.products.count %></h2>
       <h4>Products</h4>
     </div>
   </div>
  </div>
</div>
<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Latest Users</h3>
  </div>
  <div class="panel-body">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Joined</th>
      </tr>

    <tr>
      <% @supports.users.each do |user| %>
      <td><%= user.name %></td>
      <td><%= user.email %>
      <td><%= user.created_at %></td>
    </tr>
    <% end %>
    </table>

  </div>
</div>

      </div>
    </div>
  </div>
</section>
